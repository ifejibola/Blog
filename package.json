{
  "name": "articleportfolio",
  "version": "1.0.0",
  "description": "Article Blog",
  "main": "server.js",
  "proxy": "http://blog-deployer.herokuapp.com",
  "engines": {
    "node": "12.18.2",
    "npm": "7.14.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "webpack --mode=development --config webpack.config.server.js --watch",
    "dev:build-client": "webpack --mode=development --config webpack.config.client.js --watch",
    "build": "NODE_ENV=production webpack --config webpack.config.client.js && webpack --config webpack.config.server.js",
    "start": "node build/bundle.js",
    "heroku-postbuild": "npm run build",
    "test": "mocha --watch ./test ./ ./"
  },
  "keywords": [
    "Article",
    "Blog",
    "API"
  ],
  "author": "Ife Ajibola",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@fancyapps/fancybox": "^3.5.7",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.0",
    "bootstrap-validator": "^0.11.9",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto-browserify": "^3.12.0",
    "debounce": "^1.2.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-http-proxy": "^1.6.2",
    "file-loader": "^6.2.0",
    "form-data": "^4.0.0",
    "formidable": "^1.2.2",
    "multer": "^1.4.3",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "object-fit-images": "^3.2.3",
    "path-browserify": "^1.0.1",
    "postgres": "^1.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.13.0",
    "react-jss": "^10.8.1",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redis": "^3.1.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rellax": "^1.12.1",
    "response-time": "^2.3.2",
    "serialize": "^0.1.3",
    "stream-browserify": "^3.0.0",
    "uuidv4": "^6.2.12",
    "webpack": "^5.42.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "devDependencies": {
    "chai": "*",
    "css-loader": "^5.2.6",
    "mocha": "*",
    "style-loader": "^3.0.0"
  }
}
